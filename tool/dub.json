{
    "name": "tool",
    "authors": [
        "Mark"
    ],
    "description": "DXX Tools",
    "copyright": "Copyright Â© 2018, Mark",
    "license": "MIT",
    "stringImportPaths" : [ "resources" ],
    "dependencies": {
      "dxx": { "path" : "../"},
      "dxx:app": { "path" : "../"}
    },
    "subConfigurations": {
        "dxx": "dxx-lib"
    },
    "preGenerateCommands": [
        "dub fetch gen-package-version && dub run gen-package-version --build=release --arch=x86_64 -- dxx.tools --root=$PACKAGE_DIR --src=source"
    ],
    "targetPath": "bin",
    "configurations" : [
      {
          "name": "shi_sha",
          "targetName": "dale",
          "targetType": "executable",
          "targetPath": ".dub/build/shi_sha",
          "mainSourceFile": "dale.d",
          "versions" : [ "DXX_Console","DXX_Developer" ],
          "excludedSourceFiles" : [ "source/dxx/gui.d" ],
          "dependencies": {
              "dale": "~>0.0.1"
          },
      },
      {
            "name": "dxx-tool-console",
            "targetType": "executable",
            "targetName": "dxx",
            "mainSourceFile": "source/dxx/tool.d",
            "excludedSourceFiles" : [ "source/dxx/gui.d" ],
            "versions" : [ "DXX_Console" ]
        },
        {
            "name": "dxx-tool-gui",
            "targetType": "executable",
            "targetName": "dxx-g",
            "mainSourceFile": "source/dxx/gui.d",
            "excludedSourceFiles": [ "source/dxx/tool.d" ],
            "versions" : [ "DXX_Gui" ]
        }
    ]
}
