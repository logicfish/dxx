{
    "name": "tool",
    "authors": [
        "Mark"
    ],
    "description": "DXX Tools",
    "copyright": "Copyright Â© 2018, Mark Fisher",
    "license": "MIT",
    "stringImportPaths" : [ "resources" ],
    "dependencies": {
      "dxx": { "path" : "../"},
      "dxx:app": "*",
      "d-opts": "~>1.0.2"
    },
    "subConfigurations": {
        "dxx": "dxx-lib"
    },
    "-ddoxTool": "scod",
    "-ddoxFilterArgs" : [
      "properd",
      "aermicioi",
      "ctini",
      "pegged",
      "dyaml"
    ],
    "targetPath": "bin",
    "configurations" : [
      {
          "name": "shi_sha",
          "targetName": "dale",
          "targetType": "executable",
          "targetPath": ".dub/build/shi_sha",
          "mainSourceFile": "dale.d",
          "versions" : [ "DXX_Console","DXX_Developer" ],
          "excludedSourceFiles" : [
            "source/dxx/tool/gui.d",
            "source/dxx/tool/tool.d",
            "source/dxx/tools/*",
            "source/gen/dxxtool/autogen.d"
          ],
          "dependencies": {
              "dale": "~>0.0.1"
          }
      },
      {
            "name": "dxx-tool-console",
            "targetType": "executable",
            "targetName": "dxx",
            "mainSourceFile": "source/dxx/tool/tool.d",
            "excludedSourceFiles" : [ "source/dxx/tool/gui.d" ],
            "versions" : [ "DXX_Console" ]
      },
      {
            "name": "dxx-tool-gui",
            "targetType": "executable",
            "targetName": "dxx-g",
            "mainSourceFile": "source/dxx/tool/gui.d",
            "excludedSourceFiles": [ "source/dxx/tool/tool.d" ],
            "versions" : [ "DXX_Gui" ]
      },
      {
            "name": "dxx-tool-lib",
            "targetType": "library",
            "targetName": "dxx-tool",
            "excludedSourceFiles" : [
              "source/dxx/tool/gui.d",
              "source/dxx/tool/tool.d"
            ],
            "versions" : [ "DXX_Console" ]
      }
    ]
}
