{
    "name": "dxx",
    "authors": [
        "Mark"
    ],
    "description": "D language utility library and modular application framework",
    "copyright": "Copyright Â© 2018, Mark Fisher",
    "license": "MIT",
    "stringImportPaths" : [ "resources" ],
    "-ddoxTool": "scod",
    "dependencies" : {
      "ctini": "~>0.2.1",
      "semver" : "~>0.3.2"
    },
    "preGenerateCommands": [
        "dub fetch gen-package-version && dub run gen-package-version --arch=x86_64 --build=release -- dxx --root=$PACKAGE_DIR --src=source"
    ],
    "subPackages" : [
        "core",
        "app",
        "services",
        "tool",
        "tool/plugins/example",
        "examples/basic",
        "examples/plugin"
    ],
    "configurations": [
        {
            "name": "shi_sha",
            "targetName": "dale",
            "targetType": "executable",
            "targetPath": ".dub/build/shi_sha",
            "mainSourceFile": "dale.d",
            "dependencies": {
                "dale": "~>0.0.1",
                "aedi-property-reader": "~>0.2.0"
          }
        },{
            "name" : "dxx-lib",
            "targetName": "dxx-lib",
            "targetType": "library",
            "targetPath": "bin"
        }
    ]
}
