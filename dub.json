{
    "name": "dxx",
    "authors": [
        "Mark"
    ],
    "description": "D language utility library and modular application framework",
    "copyright": "Copyright Â© 2018, Mark Fisher",
    "license": "MIT",
    "stringImportPaths" : [ "resources" ],
    "-ddoxTool": "scod",
    "dependencies" : {
      "ctini": "~>0.2.1",
      "semver" : "~>0.3.2"
    },
    "subPackages" : [
        "core",
        "app",
        "tool",
        "services",
        "tool/plugins/example",
        "examples/basic",
        "examples/plugin"
    ],
    "preGenerateCommands": [
        "dub run gen-package-version --arch=x86_64 --build=release --nodeps -- dxx --root=$PACKAGE_DIR --src=source"
    ],
    "configurations": [
        {
            "name": "shi_sha",
            "targetName": "dale",
            "targetType": "executable",
            "targetPath": ".dub/build/shi_sha",
            "mainSourceFile": "dale.d",
            "dependencies": {
                "dale": "~>0.0.1",
                "aedi": "~>1.0.0"
            },
            "versions" : [ "DXX_Bootstrap" ]
        },{
            "name" : "dxx-lib",
            "targetName": "dxx-lib",
            "targetType": "library",
            "targetPath": "bin"
        },{
            "name" : "tool",
            "targetName": "dxx-tool",
            "targetType": "executable",
            "targetPath": "bin",
            "preRunCommands" : [
              "dub run --root=tool --config=shi_sha -- build"
            ],
            "mainSourceFile": "source/dxx/main.d",
            "versions" : [ "DXX_App"]
        }
    ]
}
